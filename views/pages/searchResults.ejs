<meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script src="js/search_results.js"></script>
<% layout('boilerplate') %>
<!-- Header -->
<div class="intro-header" onload='initialize();'>
    <div class="container">
        <div class="row">
            <div class="text-center">
                <div class="col-lg-12">
                    <div class="intro-message">
                        <h1 class="title-one"><%= title %></h1>
                        <hr class="intro-divider">
                        <div class="list-group">
                            <% serachResults.forEach(function(result){ %>
                            <a href=/network?net=<%= result.network_id %> class="list-group-item list-group-item-action"><%= result.network_name %></a>
                            <% }); %>
                        </div>
                    </div>
                </div>
                <!--<div id="map_canvas" class="col-lg-12" style="width:400px; height:400px" ></div>-->
            </div>
        </div>
    </div>
    <!-- /.container -->
</div>

<!-- /.intro-header -->
<a name="contact"></a>
<div class="banner">
</div>
