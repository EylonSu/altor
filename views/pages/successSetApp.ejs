<!-- === Header Section === -->
<div id="container">
	<!-- +++ Intro Section ++++ -->
    <section class="intro">
    	<div class="bg-overlay"></div>
    	<!-- Fullscreen Background Slider Images -->
		<div id="maximage">
		    <img src="Comingwa-background-slider/img/slide-one.jpg" alt="">
		    <img src="Comingwa-background-slider/img/slide-two.jpg" alt="">
		</div>

		<!-- ====+++ Intro Section Content +++==== -->
		<div class="center-content">
			<div class="intro-content text-center">
				<!-- Logo -->
				<div class="logo">
					<h2 class="text-logo">Altor</h2>
				</div><!-- /End logo -->
				<!-- ==== Main Heading ==== -->
				<h1 class="intro-title">Queued successfully<span class="obak">!</span></h1>

				<!--Spin Clock -->
				<i class="fa fa-clock-o fa-spin fa-4x fa-clock-spin"></i>

				<br></br>
				<div class="logo">
					<h2 class="text-logo"><%= branch %></h2>
				</div>
				<div class="logo">
					<h2 class="text-logo"><%= day %>, <%= month %>  <%= monthDay %>  <%= year %> </h2>
				</div><!-- /End logo -->
				 <!--====+++ Countdown Timer +++====-->
				<h1 class="intro-title">at<span class="obak"></span></h1>
				<ul class="countdown">
			        <li>
			            <span class="hours"><%= hour %></span>

			        </li>
			        <li class="seperator"><i class="fa fa-clock-o"></i><br><i class="fa fa-clock-o"></i></li>
			        <li>
			            <span class="minutes"><%= min %></span>
			        </li>

			    </ul>

				<!-- ==== Intro Section Sub-Heading ==== -->

				<div class="intro-subtitle">
					<p>Mange your life</p>
				</div>

			</div> <!-- /End Intro Section Content -->
		</div>
			<!-- ==== Intro Section Bottom Content ==== -->





				<!-- ==== Trigger Button ==== -->
			<button class="trigger" style="display: hidden">

			</button>

		<!-- /End Intro Section Bottom Content -->
	</section><!-- /end intro section -->





